<VBox prefHeight="546.0" prefWidth="754.0" styleClass="backgroundPage" stylesheets="@stylesheets.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.matheus.controleproducao.controller.CadastroProdutosController">
   <children>
